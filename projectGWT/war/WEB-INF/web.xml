<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
              http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5"
         xmlns="http://java.sun.com/xml/ns/javaee">
         
      
  <!-- Servlets -->
  <servlet>
  	<servlet-name>projServlet</servlet-name>
  	<servlet-class>com.example.project.server.ProjServiceServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>projServlet</servlet-name>
  	<url-pattern>/projectgwt/greet</url-pattern>
  </servlet-mapping>
  
  <!-- Default page t<so serve -->
  <welcome-file-list>
    <welcome-file>ProjectGWT.html</welcome-file>
  </welcome-file-list>

</web-app>